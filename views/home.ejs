<html>
<head>


  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">

  <style>
  .size{
    width:40px;
  height:25px;

  }
  </style>

</head>
<body>
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="#">WebSiteName</a>
      </div>
      <ul class=" nav navbar-nav navbar-right">
        <li ><a href='/initial_home'>Home</a></li>
        <li><a href='/downloads'>Downloads</a></li>
        <li class="active"><a href='/home'>Upload</a></li>
        <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><%=user.full_name %>     <%  if(img === 'uploads/default.png\n'){ %><img class='size' src="/uploads/default.png" /> <% }else{  %><img class='size' src=/<%=img %> /> <% } %><span class="caret"></span></a>
            <ul class="dropdown-menu">
              <li><a href=/profile/<%=user.userid %> >profile</a></li>
              <li><a href=/logout>logout</a></li>

            </ul>
          </li>


      </ul>
    </div>
  </nav>

  <div class="container">
      <h1>File upload</h1>
      <%= typeof msg!='undefined' ? msg : '' %>
      <form action="/upload" method="POST" enctype="multipart/form-data">
  <div class="row">
   <div class="file-field input-field col s12">
     <div class="btn">
       <span>File</span>
       <input name="myImage" type="file" >
     </div>
     <div class="file-path-wrapper">
       <input class="file-path validate" type="text"  >
     </div>
   </div>
  </div>
  <input name="description" placeholder="description"/>





  <div class="row">
   <div class="input-field col s12">
    <select name="year">

      <option value="1st_year" class="black-text">1st Year</option>
      <option value="2nd_year" class="black-text">2nd Year</option>
      <option value="3rd_year" class="black-text">3rd year</option>
      <option value="4th_year" class="black-text">4th year</option>
    </select>

  </div>
</div>

<div class="row">
 <div class="input-field col s12">
  <select name="branch">

    <option value="CSE" class="black-text">CSE</option>
    <option value="EE" class="black-text">EE</option>
    <option value="ME" class="black-text">ME</option>
    <option value="CE" class="black-text">CE</option>
    <option value="MEMS" class="black-text">MEMS</option>
  </select>

</div>
</div>

   <button  type="submit" class="btn">submit</button>
 </form>

  </div>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
<script>
$(document).ready(function() {
    $('select').material_select();
});
</script>



</body>
</html>
